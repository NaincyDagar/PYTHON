x=float(input("Enter your weight in kgs\n"))
y=float(input("Enter your height in meters\n"))
bmi=x/(y**2)
print("BMI=",bmi)
if bmi<18.5:
  print("underweight")
elif bmi>=18.5 and bmi <25:
  print("normal weight")
elif bmi>=25 and bmi <230:
  print("slighlt overweight")
elif bmi>=30 and bmi <35:
  print("obese")
elif bmi >=35:
  print("clinically obese")
